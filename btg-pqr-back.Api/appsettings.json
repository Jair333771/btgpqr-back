    "Email": "jair3337@hotmail.com",
    "Name": "Jair Rodriguez R",
    "Description": "This is a BTG PQR api based on .Net Core 5.0.3"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=DESKTOP-PFB2HI9;Initial Catalog=btgpqr;Integrated Security=True"
  },
  "Log4net": {
    "LogGroup": "BTGGroup",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Name": "Log4net",
    "LoggerRepository": "LogRepository",
    "OverrideCriticalLevelWith": "Fatal",
    "Watch": false,
    "PropertyOverrides": [
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file",
        "Attributes": {
          "Value": "Logger.log"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/maximumFileSize",
        "Attributes": {
          "Value": "200KB"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file"
      }
    ]
  }
}
